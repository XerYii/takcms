obj-m := xaycn.o

all:
	make -C /lib/modules/`uname -r`/build M=`pwd` modules

clean:
	make -C /lib/modules/`uname -r`/build M=`pwd` clean

install:
	make INSTALL_MOD_PATH=/lib/modules/`uname -r`/kernel/net/ipv4  modules_install
	modprobe xaycn

uninstall:
	rm /lib/modules/`uname -r`/kernel/net/ipv4/xaycn.ko
